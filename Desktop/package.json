{
  "name": "HighViz",
  "version": "1.0.0",
  "description": "Autonomous Car Vision",
  "homepage": "https://HighViz.com",
  "author": {
    "name": "Bitforge",
    "email": "bitforge.capstone@gmail.com"
  },
  "icon": "src/assets/HighViz(transparent)-white.png",
  "main": "src/electron.js",
  "type": "commonjs",
  "scripts": {
    "start": "yarn run rollup -c -w & electron .",
    "build": "rollup -c && electron-builder",
    "rebuild": "electron-rebuild -f -w bcrypt",
