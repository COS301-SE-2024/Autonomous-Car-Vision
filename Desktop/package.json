    "vite": "^5.3.4",
    "vitest": "^1.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@testing-library/svelte": "^5.2.0",
    "@vitest/ui": "^1.6.0",
    "babel-jest": "^27.0.0",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "electron-reloader": "^1.2.3",
    "ffmpeg": "^0.0.4",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-jester": "^1.0.0"
  },
  "packageManager": "yarn@4.3.1",
  "build": {
    "appId": "com.Bitforge.High-Viz",
    "files": [
      "public/build/**/*",
      "src/**/*",
      "public/index.html"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    }
